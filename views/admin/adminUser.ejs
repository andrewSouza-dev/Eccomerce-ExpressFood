<%- include('partials/menu') %>

<h2>Usuários</h2>

<table>
  <tr><th>Nome</th><th>Email</th><th>Ações</th></tr>
  <% usuarios.forEach(u => { %>
    <tr>
      <td><%= u.name %></td>
      <td><%= u.email %></td>
      <td>
        <form action="/admin/users/<%= u.id %>?_method=DELETE" method="POST">
          <button type="submit">Excluir</button>
        </form>
      </td>
    </tr>
  <% }) %>
</table>

<%- include('partials/footer') %>
